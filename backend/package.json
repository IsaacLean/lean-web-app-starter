{
  "name": "backend",
  "version": "0.2.0",
  "description": "Starter JavaScript project to kickoff development for your next web, Node.js, or native desktop app.",
  "main": "src/main.ts",
  "scripts": {
    "build": "npm run build:production",
    "build:dev": "npm run build:development",
    "build:development": "webpack --mode development",
    "build:prod": "npm run build:production",
    "build:production": "webpack --mode production",
    "format": "npm run format:check",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint \"src/**/*.+(js|json|ts)\" \"webpack/**/*.+(js|json|ts)\" webpack.config.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run start:development",
    "start:dev": "npm run start:development",
    "start:development": "nodemon --exec \"npm run build:development && node build/development/main.js\"",
    "start:prod": "npm run start:production",
    "start:production": "node build/production/main.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all --parallel typecheck lint format:check build:development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattlean/lean-js-app-starter.git",
    "directory": "backend"
  },
  "author": "Matthew Lean <matt@mattlean.com> (http://mattlean.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlean/lean-js-app-starter/issues"
  },
  "homepage": "https://github.com/mattlean/lean-js-app-starter#readme",
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "private": true
}
