{
  "name": "frontend",
  "version": "0.2.0",
  "description": "Starter JavaScript project to kickoff development for your next web, Node.js, or native desktop app.",
  "main": "src/main.tsx",
  "scripts": {
    "build": "npm run build:production",
    "build:dev": "npm run build:development",
    "build:development": "webpack --mode development",
    "build:prod": "npm run build:production",
    "build:production": "webpack --mode production",
    "format": "npm run format:check",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint \"src/**/*.+(js|json|jsx|ts|tsx)\" \"webpack/**/*.+(js|json|jsx|ts|tsx)\" webpack.config.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "nodemon --exec \"webpack serve --mode development\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all --parallel format:check lint build:production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlean/lean-js-app-starter.git"
  },
  "author": "Matthew Lean <matt@mattlean.com> (http://mattlean.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlean/lean-js-app-starter/issues"
  },
  "homepage": "https://github.com/mattlean/lean-js-app-starter#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.0.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-merge": "^5.8.0"
  },
  "private": true
}
