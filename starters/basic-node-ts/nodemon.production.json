{
    "ext": "js,json,ts",
    "ignore": [
        "src/**/__mocks__",
        "src/**/__tests__",
        "src/**/*.spec.js",
        "src/**/*.spec.ts",
        "src/**/*.test.js",
        "src/**/*.test.ts",
        "webpack.development.js"
    ],
    "watch": [
        "src/",
        "nodemon.production.json",
        "package.json",
        "tsconfig.json",
        "webpack.*.js"
    ]
}
