{
  "name": "ljas-todo-list",
  "version": "1.0.0",
  "description": "A simple todo list SPA.",
  "main": "build/development/app.js",
  "author": "",
  "scripts": {
    "build": "webpack --mode development",
    "build:debug": "node --inspect-brk=0.0.0.0:9231 node_modules/.bin/webpack --mode development",
    "build:production": "webpack --mode production",
    "build:production:debug": "node --inspect-brk=0.0.0.0:9231 node_modules/.bin/webpack --mode production",
    "build:stats": "yarn build:production -- --json > stats.json",
    "check-types": "flow",
    "dev": "nodemon --exec \"npm start\"",
    "dev:production": "nodemon --config nodemon.production.json --exec \"yarn build:production && yarn start:production\"",
    "flow-typed": "flow-typed",
    "format": "yarn format:check",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:styles:fix": "yarn lint:styles -- --fix",
    "lint-staged": "lint-staged",
    "start": "webpack serve --mode development",
    "start:production": "http-server build/production",
    "test": "jest",
    "test:coverage": "yarn test -- --coverage",
    "test:coverage:open": "open coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk=0.0.0.0:9230 node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test -- --watch || yarn test -- --watchAll"
  },
  "dependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.2",
    "cross-fetch": "^2.2.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "dotenv": "^16.4.5",
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "flow-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^5.5.1",
    "ljas-webpack": "^0.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "normalizr": "^3.6.2",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "prop-types": "^15.6.2",
    "react": "~16.7.0",
    "react-dom": "~16.7.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.9.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "babel-eslint": "^9.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-react": "~7.11.1",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.2",
    "msw": "^2.2.2",
    "nodemon": "^2.0.22",
    "prettier": "3.2.5",
    "react-test-renderer": "~16.7.0",
    "redux-mock-store": "^1.5.3",
    "snapshot-diff": "^0.10.0",
    "style-loader": "^3.3.3",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "undici": "^5.22.0",
    "webpack-dev-server": "^5.0.4"
  },
  "resolutions": {
    "cheerio": "1.0.0-rc.10",
    "enzyme-adapter-react-16/react-test-renderer": "~16.7.0"
  },
  "private": true
}
