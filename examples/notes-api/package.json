        "lint:fix": "npm run lint -- --fix",
        "lint-staged": "lint-staged",
        "prisma": "prisma",
        "prisma:studio": "npm run prisma studio -- --browser none",
        "start": "node build/server.js",
        "start:debug": "nodemon --inspect=0.0.0.0:9229 --watch build --watch src/routes/api/v1/docs/schemas build/server.js",
